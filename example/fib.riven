program {
    fn fib(n) {
      if (n = 0) {
        return 0;
      }

      if (n = 1) {
        return 1;
      }
      fib_n_1 = fib(n-1);
      fib_n_2 = fib(n-2);
      fib_n = fib_n_1 + fib_n_2;
      printf("fib(%d): %d\n", n, fib_n);
      return fib_n;
    }

    fn main() {
      fib(10);
      return 0;
    }
  }


